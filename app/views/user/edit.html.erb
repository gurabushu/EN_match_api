#エンジニア情報編集ページ


<section class="intro-wrapper">

<h1>プロフィール編集</h1>

<div class="user-intro-wrapper">
<%= form_with(model: @user, local: true) do |f| %>
    <%= f.label :name, "ユーザー名" %>
    <%= f.text_field :name %>
    <%= f.label :skill, "スキル" %>
    <%= f.text_field :skill %>
    <%= f.label :description, "自己紹介" %>
    <%= f.text_area :description %>
    <%= f.submit "更新" %>
<% end %>
</div>

<%= link_to "戻る", user_path(@user), class: "btn btn-primary" %>


</section>
